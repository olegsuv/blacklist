{
    "manifest_version": 2,
    "name": "Rieltor Blacklist",
    "description": "This extension shows status of article author searched in our database",
    "version": "1.4",
    "background": {
        "scripts": ["jquery.min.js"]
    },
    "browser_action": {
        "default_icon": "blacklist-128.png"
    },
    "permissions": [
        "activeTab",
        "https://www.olx.ua/*",
        "https://blacklist.gooffline.org.ua/*"
    ]
}

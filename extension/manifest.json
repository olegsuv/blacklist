{
    "manifest_version": 2,
    "name": "Rieltor Blacklist",
    "description": "This extension shows status of article author searched in our database",
    "version": "1.5",
    "browser_action": {
        "default_icon": "images/ic_cached_black_24dp_1x.png"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.olx.ua/*"
            ],
            "js": [
                "jquery.min.js",
                "contentScript.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "js/*",
        "images/*"
    ]
}
